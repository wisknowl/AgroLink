{
  "expo": {
    "name": "AgroBazaar",
    "slug": "AgroBazaar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/AgroBazaar.png",
    "scheme": "agrobazaar",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/AgroBazaar.png",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/AgroBazaar.png",
        "backgroundColor": "#ffffff"
        
      },
      "edgeToEdgeEnabled": true,
      "package": "com.wisknowl.AgroBazaar"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "680d5f5a-34c3-46ce-bf4f-9e37c29dafe5"
      }
    }
  }
}
